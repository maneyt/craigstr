<h2> Make a new post </h2>

<%= form_for [@category, @post] do |form| %>
  <div>
  <%= form.label :title %>
  <%= form.text_field :title %>
  </div>
  <div>
  <%= form.label :body %>
  <%= form.text_area :body %>
  </div>
  <div>
  <%= form.label :url, "Enter Image URL" %>
  <%= form.text_field :url %>
  <div>
  <%= form.submit "Post" %>
  </div>
<% end %>
